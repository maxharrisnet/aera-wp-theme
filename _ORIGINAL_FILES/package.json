{
  "name": "react-universally",
  "version": "13.0.0",
  "description": "A starter kit for universal react applications.",
  "main": "build/server/index.js",
  "engines": {
    "node": ">=7.5 <10"
  },
  "scripts": {
    "analyze:client": "babel-node internal/scripts/analyze --client",
    "analyze:server": "babel-node internal/scripts/analyze --server",
    "build": "npm run clean && cross-env NODE_ENV=production babel-node internal/scripts/build --optimize",
    "build:dev": "npm run clean && cross-env NODE_ENV=development babel-node internal/scripts/build",
    "clean": "cross-env babel-node internal/scripts/clean",
    "deploy": "babel-node internal/scripts/deploy",
    "dev": "cross-env DEPLOYMENT=development babel-node internal/development",
    "fix-sass": "npm rebuild node-sass",
    "preinstall": "node internal/scripts/preinstall",
    "postinstall": "if [ \"x$NODE_ENV\" != \"x\" ] && [ \"$NODE_ENV\" != \"development\" ]; then (npm run fix-sass && npm run build); fi;",
    "start": "cross-env NODE_ENV=production && node --use-openssl-ca build/server",
    "lint": "npm run eslint; npm run stylelint",
    "lint-staged": "lint-staged",
    "eslint": "eslint client server shared config internal",
    "stylelint": "stylelint \"shared/**/*.scss\" --syntax scss",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctrlplusb/react-universally.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "starter kit",
    "universal",
    "javascript",
    "express",
    "webpack"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ctrlplusb/react-universally/issues"
  },
  "homepage": "https://github.com/ctrlplusb/react-universally#readme",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleDirectories": [
      "node_modules",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/internal/jest/styleMock.js",
      "\\.(je?pg|png|gif|ico|otf|ttf|eot|woff|woff2|svg|webp)$": "<rootDir>/internal/jest/assetMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(build|internal|node_modules|flow-typed|public)/"
    ]
  },
  "pre-commit": "lint-staged",
  "lint-staged": {},
  "dependencies": {
    "app-root-dir": "1.0.2",
    "assets-webpack-plugin": "3.9.7",
    "autoprefixer": "6.7.7",
    "axios": "^0.21.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-jsx-self": "6.22.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-template": "6.26.0",
    "basic-auth": "^1.1.0",
    "body-parser": "^1.18.3",
    "bodymovin": "4.13.0",
    "bxslider": "^4.2.14",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "check-node-version": "^4.1.0",
    "chokidar": "1.6.1",
    "classnames": "2.2.6",
    "classnames-loader": "2.0.0",
    "colors": "1.3.2",
    "compression": "1.7.3",
    "cors": "^2.8.4",
    "cross-env": "4.0.0",
    "css-loader": "0.28.1",
    "dotenv": "4.0.0",
    "express": "4.16.4",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "2.0.0",
    "glob": "7.1.3",
    "gsap": "^2.0.2",
    "happypack": "3.0.3",
    "helmet": "3.14.0",
    "hpp": "0.2.2",
    "html-webpack-plugin": "2.28.0",
    "jquery": "^3.5.0",
    "json-stringify-safe": "5.0.1",
    "lodash": "^4.17.11",
    "md5": "2.2.1",
    "mobx": "3.1.9",
    "mobx-react": "4.1.8",
    "mobx-react-devtools": "4.2.15",
    "node-sass": "^4.9.4",
    "object.entries": "^1.0.4",
    "offline-plugin": "4.7.0",
    "postcss": "^6.0.8",
    "postcss-csso": "2.0.0",
    "postcss-loader": "2.0.6",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "react-async-bootstrapper": "2.1.1",
    "react-async-component": "1.0.2",
    "react-dom": "16.5.2",
    "react-ga": "^2.5.3",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.3.11",
    "react-jobs": "1.0.1",
    "react-markdown": "^2.5.1",
    "react-overlays": "^0.8.3",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-scroll": "^1.8.2",
    "react-share": "^4.1.0",
    "react-sticky": "^6.0.3",
    "react-waypoint": "^8.0.3",
    "redis": "^3.1.2",
    "redis-streams": "^1.1.0",
    "regenerator-runtime": "0.10.5",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.3",
    "semver": "5.6.0",
    "serialize-javascript": "4.0.0",
    "source-map-support": "0.4.15",
    "stats.js": "0.17.0",
    "style-loader": "0.17.0",
    "svg-to-jsx-loader": "2.0.1",
    "three": "^0.85.0",
    "three-orbit-controls": "82.1.0",
    "user-home": "2.0.0",
    "uuid": "3.3.2",
